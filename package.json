{
  "name": "ember-cli-deploy-yapp-pack",
  "version": "7.0.0",
  "description": "A pack of ember-cli-deploy plugins for deploying Yapp's Ember apps.",
  "scripts": {
    "lint:js": "eslint .",
    "release": "release-it"
  },
  "engines": {
    "node": "14.* || 16.* || 18.* || >= 20.*"
  },
  "devDependencies": {
    "ember-cli": "~3.28.3",
    "eslint": "^8.0.1",
    "lerna-changelog": "^2.2.0",
    "release-it": "14.11.6",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin-pack"
  ],
  "repository": "https://github.com/yappbox/ember-cli-deploy-yapp-pack",
  "license": "MIT",
  "author": "Luke Melia and the Yapp team",
  "dependencies": {
    "ember-cli-deploy-lightning-pack": "^6.0.0",
    "ember-cli-deploy-sentry-cli": "yappbox/ember-cli-deploy-sentry-cli#80ab41a732e17fb0f0a597987766a058fe227674",
    "ember-cli-deploy-slack": "^2.0.0",
    "heroku-client": "^3.1.0",
    "rsvp": "^4.8.5"
  },
  "ember-addon": {
    "defaultBlueprint": "yapp-deploy-config"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "v${version}",
      "pushArgs": "--follow-tags --no-verify"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    }
  },
  "volta": {
    "node": "20.19.3",
    "yarn": "1.22.22"
  }
}
