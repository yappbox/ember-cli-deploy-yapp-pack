{
  "name": "ember-cli-deploy-yapp-pack",
  "version": "6.3.0",
  "description": "A pack of ember-cli-deploy plugins for deploying Yapp's Ember apps.",
  "scripts": {
    "lint:js": "eslint .",
    "release": "release-it"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "devDependencies": {
    "ember-cli": "~3.28.3",
    "eslint": "^8.0.1",
    "lerna-changelog": "^2.2.0",
    "release-it": "14.11.6",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin-pack"
  ],
  "repository": "https://github.com/yappbox/ember-cli-deploy-yapp-pack",
  "license": "MIT",
  "author": "Luke Melia and the Yapp team",
  "dependencies": {
    "ember-cli-deploy-lightning-pack": "^4.2.2",
    "ember-cli-deploy-slack": "^1.0.1",
    "heroku-client": "^3.1.0",
    "rsvp": "^4.8.5"
  },
  "ember-addon": {
    "defaultBlueprint": "yapp-deploy-config"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "v${version}",
      "pushArgs": "--follow-tags --no-verify"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    }
  },
  "volta": {
    "node": "14.18.1",
    "yarn": "1.22.17"
  }
}
